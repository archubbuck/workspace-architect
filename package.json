{
  "name": "github-copilot-packed",
  "version": "1.1.0",
  "description": "A collection of instructions, prompts, and chat modes for GitHub Copilot",
  "bin": {
    "github-copilot-packed": "./bin/cli.js"
  },
  "scripts": {
    "start:registry": "verdaccio --config verdaccio/config.yaml",
    "test:local": "node bin/cli.js list && node bin/cli.js download instructions:basic-setup --dry-run",
    "sync": "node scripts/sync.js",
    "release": "release-it",
    "publish:local": "npm run release && npm publish --registry http://localhost:4873"
  },
  "keywords": [
    "copilot",
    "instructions",
    "prompts",
    "chatmodes",
    "collections"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "fs-extra": "^11.2.0",
    "gray-matter": "^4.0.3",
    "inquirer": "^9.2.15"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^8.0.2",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "release-it": "^17.11.0",
    "verdaccio": "^5.29.0"
  },
  "type": "module"
}
